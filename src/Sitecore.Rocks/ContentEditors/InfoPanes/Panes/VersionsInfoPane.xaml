<UserControl x:Class="Sitecore.Rocks.ContentEditors.InfoPanes.Panes.VersionsInfoPane" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:i="clr-namespace:Sitecore.Rocks.ContentEditors.InfoPanes" mc:Ignorable="d" d:DesignHeight="300"
    d:DesignWidth="300">
    <WrapPanel Orientation="Vertical">
        <i:InfoPaneListBox x:Name="Languages" Header="Language" SelectionChanged="SetLanguage" />
        <i:InfoPaneListBox x:Name="Versions" Header="Version" SelectionChanged="SetVersion" />
    </WrapPanel>
</UserControl>